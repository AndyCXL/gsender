.location-wrapper {
    display: flex;
    justify-content: center;
    align-items: inherit;

    .buttonwrap {
        margin-top: 1vh;
    }

    .displaypanelTable {
        margin-left: -5%;
    }

    .control-buttons {
        align-items: center;
        align-content: center;
        align-self: center;
    }

    .endStopsActive {
        display: block;
        align-self: center;
        width: 25%;
        margin-left: -1vw;
        margin-right: 1vw;
    }

    .endStopsDisabled {
        display: none;
    }

    .endStopActiveControls {
        padding: 1%;
        align-content: space-around;
        flex-wrap: wrap;
        display: flex;
        width: 30%;
        margin-right: -2vw;
        margin-top: -2%;
        display: flex;
        justify-content: center;
    }

    .buttoncontainer {
        border: 5px solid black;
        display: flex;
    }

    .quicktravel {
        width: 100%;
        max-width: 10rem;
        font-size: 14px;
        margin: 0;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .centerhomebutton {
        white-space: nowrap;
        text-align: center;
        border: solid 1px #9ca3af;
        color: #3e85c7;
        border-radius: .25rem;
        background-color: rgba(209, 213, 219, .6);
        display: flex;
        justify-content: center;
        -webkit-box-pack: center;
        align-items: center;
        --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
        -webkit-box-shadow: var(--tw-ring-offset-shadow, 0 0 rgba(0, 0, 0, 0)), var(--tw-ring-shadow, 0 0 rgba(0, 0, 0, 0)), var(--tw-shadow);
        box-shadow: var(--tw-ring-offset-shadow, 0 0 rgba(0, 0, 0, 0)), var(--tw-ring-shadow, 0 0 rgba(0, 0, 0, 0)), var(--tw-shadow);
        padding: .25rem;
        transition: 200ms ease-in-out;
        font-size: .9rem;
        font-weight: normal;

        &:hover {
            border: 1px solid #3e85c7;
            transition: 200ms ease-in-out;
            background-color: #E5E7EB;
            border-radius: .25rem;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }

        &:disabled {
            color: #6F7376;
            background-color: #D9DCE1;
            border: 1px solid #6F7376;
            cursor: no-drop;
            font-size: 14px;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }
    }

    .backleft {
        background-color: #3e85c7;
        clip-path: polygon(0 0, 100% 0, 100% 40%, 40% 40%, 40% 100%, 0 100%);
        width: 30%;
        height: 30%;
        justify-content: flex-start;
        margin-top: 4px;
        display: flex;
        border: none;
        outline: none;
        vertical-align: top;
        text-align: left;
        --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
        box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);

        &disabled {
            background-color: #3e85c7;
            clip-path: polygon(0 0, 100% 0, 100% 40%, 40% 40%, 40% 100%, 0 100%);
            width: 30%;
            height: 30%;
            justify-content: flex-start;
            margin-top: 4px;
            display: flex;
            border: none;
            outline: none;
            vertical-align: top;
            text-align: left;
            opacity: .6;
            cursor: no-drop;
            pointer-events: none;
        }

        &:hover {
            background-color: #5191cc;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }
    }

    .bltext {
        align-self: flex-start;
        color: white;
        margin-top: 2rem;
        margin-left: -1px;
        position: absolute;
        display: flex;
        font-size: 1rem;
    }

    .blhomeicon {
        color: white;
        font-size: 18px;
        margin-left: -3px;
    }

    .backRight {
        background-color: #3e85c7;
        clip-path: polygon(0 0, 100% 0, 100% 100%, 60% 100%, 60% 40%, 0 40%);
        margin-left: auto;
        border: none;
        display: flex;
        margin-top: 4px;
        outline: none;
        justify-content: flex-end;
        width: 30%;
        height: 30%;

        &disabled {
            background-color: #3e85c7;
            clip-path: polygon(0 0, 100% 0, 100% 100%, 60% 100%, 60% 40%, 0 40%);
            margin-left: auto;
            border: none;
            display: flex;
            margin-top: 4px;
            outline: none;
            justify-content: flex-end;
            width: 30%;
            height: 30%;
            opacity: .6;
            cursor: no-drop;
            pointer-events: none;
        }

        &:hover {
            background-color: #5191cc;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }
    }

    .brtext {
        display: flex;
        color: white;
        margin-right: -2px;
        font-size: 1rem;
        margin-top: 2rem;
        justify-content: flex-end;
    }

    .brhomeicon {
        color: #fff;
        font-size: 18px;
        margin-right: -16px;
    }

    .frontleft {
        clip-path: polygon(0 0, 40% 0, 40% 60%, 100% 60%, 100% 100%, 0 100%);
        background-color: #3e85c7;
        display: flex;
        width: 30%;
        height: 30%;
        justify-content: flex-start;
        align-items: normal;
        margin-top: auto;
        margin-left: 0;
        border: none;
        outline: none;

        &disabled {
            clip-path: polygon(0 0, 40% 0, 40% 60%, 100% 60%, 100% 100%, 0 100%);
            background-color: #3e85c7;
            display: flex;
            width: 30%;
            height: 30%;
            justify-content: flex-start;
            align-items: normal;
            margin-top: auto;
            margin-left: 0;
            border: none;
            outline: none;
            opacity: .6;
            cursor: no-drop;
            pointer-events: none;
        }

        &:hover {
            background-color: #5191cc;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }
    }

    .fltext {
        display: flex;
        justify-content: flex-start;
        margin-left: -1px;
        margin-top: 2rem;
        color: #fff;
        font-size: 1rem;
    }

    .flhomeicon {
        color: #fff;
        font-size: 18px;
        margin-left: -16px;
        margin-top: 62px;
    }

    .frontright {
        clip-path: polygon(60% 60%, 60% 0, 100% 0, 100% 100%, 0 99%, 0 60%);
        background-color: #3e85c7;
        display: flex;
        align-items: flex-end;
        justify-content: flex-end;
        border: none;
        outline: none;
        margin-left: auto;
        margin-top: auto;
        width: 30%;
        height: 30%;

        &disabled {
            clip-path: polygon(60% 60%, 60% 0, 100% 0, 100% 100%, 0 99%, 0 60%);
            background-color: #3e85c7;
            display: flex;
            align-items: flex-end;
            justify-content: flex-end;
            border: none;
            outline: none;
            margin-left: auto;
            margin-top: auto;
            width: 30%;
            height: 30%;
            opacity: .6;
            cursor: no-drop;
            pointer-events: none;
        }

        &:hover {
            background-color: #5191cc;
            --tw-shadow: 0 4px 6px -1px rgba(0, 0, 0, .1), 0 2px 4px -1px rgba(0, 0, 0, .06);
            box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
        }
    }

    .frtext {
        display: flex;
        justify-content: flex-end;
        color: #fff;
        margin-bottom: 2rem;
        margin-right: -1rem;
        font-size: 1rem;
    }

    .frhomeicon {
        color: #fff;
        font-size: 18px;
        margin-right: -5px;
    }
}
